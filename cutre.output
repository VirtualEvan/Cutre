Grammar

    0 $accept: programa $end

    1 programa: cabecera bloque

    2 cabecera: PROGRAMA IDENTIFICADOR ';' ficheros_cabecera

    3 ficheros_cabecera: %empty
    4                  | ficheros_cabecera fichero_cabecera

    5 fichero_cabecera: CABECERA PATH ';'

    6 bloque: declaraciones_tipos declaraciones_constantes declaraciones_variables declaraciones_funciones bloque_instrucciones

    7 declaraciones_tipos: %empty
    8                    | TIPOS lista_declaraciones_tipo FIN

    9 lista_declaraciones_tipo: declaracion_tipo
   10                         | lista_declaraciones_tipo declaracion_tipo

   11 declaracion_tipo: IDENTIFICADOR ES especificacion_tipo ';'

   12 especificacion_tipo: referencias tipo_basico

   13 referencias: %empty
   14            | referencias REF

   15 tipo_basico: IDENTIFICADOR
   16            | tipo_escalar
   17            | tipo_enumerado
   18            | tipo_estructurado

   19 tipo_escalar: ENTERO
   20             | REAL
   21             | CARACTER
   22             | CADENA
   23             | FICHERO

   24 tipo_enumerado: ARRAY especificacion_tipo
   25               | HASH especificacion_tipo
   26               | CONJUNTO especificacion_tipo

   27 tipo_estructurado: ESTRUCTURA PRINCIPIO lista_campos FIN
   28                  | UNION PRINCIPIO lista_campos FIN

   29 lista_campos: linea_campo
   30             | lista_campos linea_campo

   31 linea_campo: lista_identificadores ES especificacion_tipo ';'

   32 lista_identificadores: IDENTIFICADOR
   33                      | lista_identificadores ',' IDENTIFICADOR

   34 declaraciones_constantes: CONSTANTES declaracion_constante FIN

   35 declaracion_constante: IDENTIFICADOR ES tipo_basico '=' constante ';'
   36                      | declaracion_constante IDENTIFICADOR ES tipo_basico '=' constante ';'

   37 constante: CTC_ENTERA
   38          | CTC_REAL
   39          | CTC_CARACTER
   40          | CTC_CADENA
   41          | constante_enumerada
   42          | constante_estructurada

   43 constante_enumerada: '(' constante_completa ')'
   44                    | '(' hash_completa ')'
   45                    | '(' ')'

   46 constante_completa: constante
   47                   | constante_completa ',' constante

   48 hash_completa: elemento_hash
   49              | hash_completa ',' elemento_hash

   50 elemento_hash: CTC_CADENA FLECHA_DOBLE constante

   51 constante_estructurada: '{' campo_constante '}'
   52                       | '{' campo_const_coma campo_constante '}'

   53 campo_const_coma: campo_constante ','
   54                 | campo_const_coma campo_constante ','

   55 campo_constante: IDENTIFICADOR '=' constante

   56 declaraciones_variables: VARIABLES declaracion_variables FIN

   57 declaracion_variables: lista_identificadores ':' especificacion_tipo ';'
   58                      | declaracion_variables lista_identificadores ':' especificacion_tipo ';'

   59 declaraciones_funciones: declaracion_funcion
   60                        | declaraciones_funciones declaracion_funcion

   61 declaracion_funcion: FUNCION IDENTIFICADOR FLECHA_SIMPLE tipo_salida cuerpo_funcion
   62                    | FUNCION IDENTIFICADOR lista_parametros FLECHA_SIMPLE tipo_salida cuerpo_funcion

   63 lista_parametros: '(' conj_parametros parametros ')'

   64 conj_parametros: parametros ';'
   65                | conj_parametros parametros ';'

   66 parametros: lista_identificadores ':' especificacion_tipo

   67 tipo_salida: especificacion_tipo
   68            | NADA

   69 cuerpo_funcion: bloque_instrucciones
   70               | declaraciones_funciones bloque_instrucciones
   71               | declaraciones_variables bloque_instrucciones
   72               | declaraciones_variables declaraciones_funciones bloque_instrucciones
   73               | declaraciones_constantes bloque_instrucciones
   74               | declaraciones_constantes declaraciones_funciones bloque_instrucciones
   75               | declaraciones_constantes declaraciones_variables bloque_instrucciones
   76               | declaraciones_constantes declaraciones_variables declaraciones_funciones bloque_instrucciones

   77 bloque_instrucciones: PRINCIPIO instrucciones FIN

   78 instrucciones: instruccion
   79              | instrucciones instruccion

   80 instruccion: instruccion_expresion
   81            | instruccion_bifurcacion
   82            | instruccion_bucle
   83            | instruccion_salto
   84            | instruccion_destino_salto
   85            | instruccion_devolver

   86 instruccion_expresion: expresion_funcional ';'
   87                      | asignacion ';'

   88 asignacion: expresion_indexada operador_asignacion expresion

   89 operador_asignacion: '='
   90                    | POT_ASIG
   91                    | MULT_ASIG
   92                    | DIV_ASIG
   93                    | MOD_ASIG
   94                    | SUMA_ASIG
   95                    | RESTA_ASIG
   96                    | DESPI_ASIG
   97                    | DESPD_ASIG
   98                    | AND_ASIG
   99                    | XOR_ASIG
  100                    | OR_ASIG

  101 instruccion_bifurcacion: SI '(' expresion ')' accion otros_casos FIN
  102                        | SI '(' expresion ')' accion otros_casos SINO accion FIN

  103 otros_casos: %empty
  104            | otros_casos SI_ENCAMBIO '(' expresion ')' accion

  105 accion: instruccion
  106       | bloque_instrucciones

  107 instruccion_bucle: MIENTRAS '(' expresion ')' accion
  108                  | HAZ accion MIENTRAS '(' expresion ')' ';'
  109                  | PARA '(' ';' expresion ';' lista_asignaciones ')' accion
  110                  | PARA '(' lista_asignaciones ';' expresion ';' lista_asignaciones ')' accion
  111                  | PARA_CADA IDENTIFICADOR '(' expresion ')' accion

  112 lista_asignaciones: asignacion
  113                   | lista_asignaciones ',' asignacion

  114 instruccion_salto: IR_A IDENTIFICADOR ';'
  115                  | CONTINUAR ';'
  116                  | ESCAPE ';'

  117 instruccion_destino_salto: ETIQUETA IDENTIFICADOR ';'

  118 instruccion_devolver: DEVOLVER ';'
  119                     | DEVOLVER expresion ';'

  120 expresion_constante: CTC_ENTERA
  121                    | CTC_REAL
  122                    | CTC_CADENA
  123                    | CTC_CARACTER

  124 expresion_indexada: expresion_basica
  125                   | expresion_indexada '.' expresion_basica
  126                   | expresion_indexada FLECHA_SIMPLE expresion_basica
  127                   | expresion_indexada indice
  128                   | expresion_indexada FLECHA_SIMPLE indice

  129 expresion_basica: IDENTIFICADOR
  130                 | '(' expresion ')'
  131                 | '^' expresion_basica
  132                 | '\\' expresion_basica

  133 indice: '[' expresion ']'
  134       | '{' expresion '}'

  135 expresion_funcional: IDENTIFICADOR '(' ')'
  136                    | IDENTIFICADOR '(' lista_expresiones ')'

  137 lista_expresiones: expresion
  138                  | lista_expresiones ',' expresion

  139 expresion: expresion_logica
  140          | expresion_logica '?' expresion ':' expresion

  141 expresion_logica: expresion_logica AND prioridad8
  142                 | expresion_logica OR prioridad8
  143                 | prioridad8

  144 prioridad8: prioridad8 GE prioridad7
  145           | prioridad8 LE prioridad7
  146           | prioridad8 EQ prioridad7
  147           | prioridad8 NEQ prioridad7
  148           | prioridad7

  149 prioridad7: prioridad7 '&' prioridad6
  150           | prioridad7 '@' prioridad6
  151           | prioridad7 '|' prioridad6
  152           | prioridad7 '>' prioridad6
  153           | prioridad7 '<' prioridad6
  154           | prioridad6

  155 prioridad6: prioridad6 DESPI prioridad5
  156           | prioridad6 DESPD prioridad5
  157           | prioridad5

  158 prioridad5: prioridad5 '+' prioridad4
  159           | prioridad5 '-' prioridad4
  160           | prioridad4

  161 prioridad4: prioridad4 '*' prioridad3
  162           | prioridad4 '/' prioridad3
  163           | prioridad3

  164 prioridad3: prioridad3 POTENCIA prioridad2
  165           | prioridad2

  166 prioridad2: TAMANO prioridad1
  167           | '!' prioridad1
  168           | '~' prioridad1
  169           | '-' prioridad1
  170           | prioridad1

  171 prioridad1: expresion_constante
  172           | expresion_funcional
  173           | expresion_indexada


